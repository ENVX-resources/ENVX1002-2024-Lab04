ggplot(data.frame(x = c(4.16-4*0.3, 4.16+4*0.3)), aes(x = x)) +
stat_function(fun = dnorm, args = list(mean = 4.16, sd=0.30)
,geom = "area", fill = "white") +
stat_function(fun = dnorm, args = list(mean = 4.16, sd=0.30)
, xlim = c(4.16-4*0.3, 4), geom = "area", fill = "red") +
xlab("x") +
ylab(expression(N(4.16,0.30^2)~pdf))
